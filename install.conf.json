[
  { "defaults": { "link": { "relink": true } } },
  { "clean": ["~"] },
  {
    "link": {
      "~/.SpaceVim.d": "SpaceVim.d",
      "~/.Spacevim": "SpaceVim",
      "~/.config/nvim": "SpaceVim",
      "~/.zshrc": "zsh/zshrc",
      "~/.zprofile": "zsh/zprofile",
      "~/.zlogin": "zsh/zlogin"
    }
  },
  {
    "shell": [
      ["git submodule update --init --recursive", "Installing submodules"],
      ["cd ./SpaceVim/bundle/vimproc.vim/ && make", "Building vimproc manually"]
    ]
  }
]
